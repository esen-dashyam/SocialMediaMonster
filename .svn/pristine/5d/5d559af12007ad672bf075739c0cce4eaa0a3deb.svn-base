
* {
  margin: 0;
  padding: 0;
  box-sizing: border-box;
}
html {
  scroll-behavior: smooth;
}
body {
  font-family: "ROBOTO", sans-serif;
}

.header{
  width: 100%;
  position: relative;
}
.logo img{
  height: 60px;
}
/***********Navigation**************/
#menu-btn {
    display: none;
}


/***********Header Area**************/
/***********Subject**************/
.subject{
  background-size: 1000px;
  background-position: center;
}
.subject-heading{
  margin-top: 60px;
  display: flex;
  justify-content: center;
  align-items: center;
  text-align: center;
  flex-direction: column;
}
.subject-heading h2{
  line-height: 55px;
  font-size: 2.25rem;
  color: #222;
  letter-spacing: 1px;
  font-weight: 700;
  margin: 0px;
}
.subject-heading p{
  font-size: 1rem;
  color: #777;
  width: 50%;
  padding-bottom: 20px;
}
.box-container{
  display: flex;
  justify-content: center;
  align-items: center;
  flex-wrap: wrap;
  margin: 20px 30px;
  height: 768px;
}

.box {
    display: flex;
    justify-content: center;
    align-items: center;
    flex-direction: column;
    width: 300px;
    height: 400px;
    text-align: center;
    border: 1px solid #E3EBF6;
    border-radius: 10px;
    margin: 20px;
    flex-grow: 1;
}
.box-container img {
    display: inline-block;
    /*  max-width: 500px;*/
    width: 100%;
    height: 300px;
    margin: 10px;
    border-top: 1px solid #E3EBF6
}
.box font{
  box-sizing: 1.5rem;
  color: #222;
  letter-spacing: 1px;
  font-weight: 700;
}
.box p{
  font-size: 1rem;
  color: #777;
  display: block;
  display: -webkit-box;
  --webkit-line-clamp: 3;
  --webkit-box-orient: vertical;
  overflow: hidden;
  text-overflow: ellipsis;
  max-width: 500px;
}
.box a{
  width: 150px;
  height: 40px;
  display: flex;
  justify-content: center;
  align-items: center;
  border:2px solid #ff0266;
  text-transform: uppercase;
  margin: auto;
  border-radius: 5px;
  font-weight: 600;
  font-size: 0.9rem;
  color: #ff0266;
  margin: 50px;
  transition: 00.4s ease-in-out;
}
.box a:hover{
  background-color: #ff0266;
  color: #f1f6f5;
  box-shadow: 2px 2px 20px rgba(2, 238, 255, 0.336);
}


/*****************ABOUT ****************/
.about{
  width: 100%;
  height: 768px;
  position: relative;
}
.about-content{
  display: flex;
  justify-content: space-around;
  align-items: center;
  position: absolute;
  left: 50%;
  top: 50%;
  transform: translate(-50%,-50%);
  width: 90%;
}
.about-image{
  width: 500px;
  height: 400px;
  margin: 20px;
}
.about-image img{
  width: 100%;
  height: 100%;
  object-fit: contain;
  object-position: center;
}
.about-text{
  width: 500px;
  margin: 20px;
}
.about-text h1{
  font-size: 3.5rem;
  line-height: 55px;
  color: #222;
  letter-spacing: 1px;
  font-weight: 700;
  margin: 0px;
}
.about-text p{
  font-size: 1rem;
  color: #777;
  padding-bottom: 10px;
}
.about-text span{
  color: #ff0266;
  text-decoration: underline;
}
.about-btn{
  width: 120px;
  height: 40px;
  display: flex;
  justify-content: space-around;
  align-items: center;
  background-color: #ffffff;
  color: #ff0266;
  border: 2px solid #ff0266;
  text-transform: uppercase;
  transition: 0.4s ease-in-out;
}
.about-btn:hover{
  background-color: #ff0266;
  color: #ffffff;
}

/****** Nav pink***********/
.navpink,
.slider {
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  height: 100vh;
  position: relative;
  background: linear-gradient(to bottom left, #fafcff5e, #ffffff);
  /* background-color: #1e1f26; 
  background-color: #2a2a2c;
  background-image: url(images/background.png);*/
  background-repeat: round;
  text-align: center;
  padding: 0 2em;
}

.navpink h1,
.slider h1 {
  font-family: "Josefin Sans", sans-serif;
  font-size: 5vw;
  margin: 0;
  padding-bottom: 0.5rem;
  letter-spacing: 0.3rem;
  color: #150f2ca2;
  transition: all 0.3s ease;
  z-index: inherit;
}
h1:hover {
  transform: translate3d(0, -10px, 22px);
  color: #ff0266;
}

.slider h2 {
  font-size: 2vw;
  letter-spacing: 0.3rem;
  font-family: "ROBOTO", sans-serif;
  font-weight: 300;
  color: #faebd7;
  z-index: initial;
}
h3.span {
  font-size: 2vw;
  letter-spacing: 0.10rem;
  font-family: "ROBOTO", sans-serif;
  font-weight: 300;
  color: #faebd7;
  z-index: 4;
}

a {
  text-decoration: none;
}
.background {
  position: absolute;
  height: 100vh;
  top: 0;
  bottom: 0;
  left: 0;
  right: 0;
  z-index: 0;
}
.loader span {
  color: #ceb697;
  text-shadow: 0 0 0 #f5f2ee;
  -webkit-animation: loading 3.5s ease-in-out infinite alternate;
}

